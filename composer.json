{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "backpack/calendar-operation": "^1.0",
        "backpack/crud": "^6.7",
        "backpack/editable-columns": "^3.0",
        "backpack/permissionmanager": "^7.2",
        "backpack/pro": "^2.2",
        "backpack/theme-tabler": "^1.2",
        "barryvdh/laravel-dompdf": "^3.0",
        "barryvdh/laravel-snappy": "^1.0",
        "doctrine/dbal": "^4.2",
        "intervention/image": "^3.9",
        "intervention/image-laravel": "^1.3",
        "kreait/laravel-firebase": "^6.0",
        "laravel/framework": "^11.9",
        "laravel/passport": "^12.4",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "~3.0",
        "livewire/livewire": "^3.5",
        "maatwebsite/excel": "^3.1",
        "mehedijaman/laravel-zkteco": "^1.0",
        "predis/predis": "^2.3",
        "pusher/pusher-php-server": "^7.2",
        "spatie/laravel-medialibrary": "^11.10",
        "zircote/swagger-php": "3.*"
    },
    "require-dev": {
        "backpack/generators": "^4.0",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "post-install-cmd": ["@php artisan storage:link --quiet"]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "backpack": {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        },
        "backpack": {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        }
    }
}
